<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wxk.leads.myrecyclerview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".use.BasicActivity"/>
        <activity android:name=".use.CommonActivity"/>
        <activity android:name=".use.MultiActivity"/>
        <activity android:name=".use.HeaderFooterActivity"/>
        <activity android:name=".use.DragActivity"/>
        <activity android:name=".use.LoadRefreshActivity"/>
    </application>

</manifest>